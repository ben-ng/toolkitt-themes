<div class="sliderWrap">
  <ul class="carousel elastislide-list">
  {{#each media}}
    <li>
      <a href="{{playerUrl}}" class="app">
        <img src="{{thumbnailUrl}}" alt="{{name}}" />
      </a>
    </li>
  {{/each}}
  </ul>
</div>
<div class="controls clearfix centered">
  <a href="#" class="carouselControl bigClose pull-left">&times;</a>
  <a href="#" class="carouselControl reelToggle"><span class="symbol">&and;</span><br /><small>Reel</small></a>
</div>